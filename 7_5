#include<stdio.h>
int main()
{
    int i,j,k,gcd,lcm;
    printf("Enter 2 no's ;");
    scanf("%d%d",&j,&k);

    for (i=1;i<=j && i<=k;i++)
        
    {
       if(j%i==0 && k%i==0)
        gcd=i;
       }
       lcm=j*k/gcd;
       printf("The GCD is:%d\n",gcd);
       printf("The LCM IS :%d\n",lcm);
    


}
